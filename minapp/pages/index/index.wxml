<view class="yapp"><view class="ypage"><view class="ypagebody"><cu-custom vue-id="1" bgColor="{{bgcolor}}" isBack="{{false}}" bind:__l="__l" vue-slots="{{['left','content','right']}}"><view slot="left"><view class="text-gray topshare"><block wx:if="{{total}}"><block><text class="cuIcon-profile"></text>{{''+total}}</block></block><block wx:if="{{shared}}"><button class="cu-btn" open-type="share"><text class="cuIcon-forward margin-left"></text>{{''+shared+''}}</button></block></view></view><view slot="content"></view><view slot="right"></view></cu-custom><block wx:if="{{!start_answer}}"><view><block wx:if="{{appstartlogo}}"><view class="text-center"><image style="height:100px;" src="{{appstartlogo}}" mode="aspectFill" class="_img"></image></view></block><view class="text-center text-xl padding text-bold">{{''+title+''}}</view><view class="padding text-left "><block wx:for="{{rules}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="text-gray" index="{{index}}">{{''+(index+1)+"."+item+''}}</view></block></view><view class="padding text-left text-red">{{remarks}}</view><view class="padding flex flex-direction"><button data-event-opts="{{[['tap',[['start']]]]}}" class="cu-btn line-green lg" bindtap="__e">开始答题</button><button class="margin-top cu-btn line-gray lg" open-type="share">考考好友</button><view class="contactbox padding text-right text-gray"><button class="cu-btn content text-red" open-type="contact"><text>在线客服</text><text class="cuIcon-message"></text></button></view></view><block wx:if="{{recommends.length>0}}"><view class="margin-top"><view class="cu-bar"><view class="action">应用推荐：</view></view><view class="cu-list grid no-border col-5"><block wx:for="{{recommends}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['click',['$0'],[[['recommends','',index]]]]]]]}}" class="cu-item " bindtap="__e"><view><image class="cu-avatar round" src="{{item.logo}}" mode="aspectFit"></image></view><text class="text-cut">{{item.name}}</text></view></block></view></view></block></view></block></view><view class="yfoot"><support vue-id="2" bind:__l="__l"></support></view></view></view>